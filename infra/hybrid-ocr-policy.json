{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3Access",
      "Effect": "Allow",
      "Action": ["s3:PutObject","s3:GetObject","s3:DeleteObject"],
      "Resource": ["arn:aws:s3:::hybrid-ocr-dev-uploads-1770938266/*"]
    },
    {
      "Sid": "S3ListBucket",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": ["arn:aws:s3:::hybrid-ocr-dev-uploads-1770938266"]
    },
    {
      "Sid": "SQSAccess",
      "Effect": "Allow",
      "Action": ["sqs:SendMessage","sqs:ReceiveMessage","sqs:DeleteMessage","sqs:ChangeMessageVisibility","sqs:GetQueueAttributes","sqs:GetQueueUrl"],
      "Resource": "arn:aws:sqs:ap-southeast-1:037931721217:hybrid-ocr-dev-jobs"
    },
    {
      "Sid": "DDBAccess",
      "Effect": "Allow",
      "Action": ["dynamodb:PutItem","dynamodb:GetItem","dynamodb:UpdateItem"],
      "Resource": "arn:aws:dynamodb:ap-southeast-1:037931721217:table/hybrid-ocr-dev-jobs"
    }
  ]
}
